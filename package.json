{
  "name": "tradinglogic-deribit",
  "version": "0.1.1",
  "description": "Implementation of trading logic using Deribit data for BTC futures.",
  "main": "./src/app.js",
  "scripts": {
    "pretest": "npm install",
    "test": "jest",
    "testdebug": "node --unhandled-rejections=strict --trace-warnings --expose-gc node_modules/.bin/jest --no-cache --detectOpenHandles --runInBand",
    "killall": "killall node && killall jest",
    "startdebug": "node --trace-warnings --expose-gc --max_old_space_size=4096 ./src/app.js",
    "start": "node --max_old_space_size=4096 ./src/app.js",
    "preversion": "npm run test",
    "version": "",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisqueen/tradinglogic-deribit.git"
  },
  "author": "Christopher Queen<christopher.queen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisqueen/tradinglogic-deribit/issues"
  },
  "homepage": "https://github.com/chrisqueen/tradinglogic-deribit#readme",
  "keywords": [
    "deribit",
    "tradingview",
    "crypto",
    "exchange",
    "bitcoin",
    "btc"
  ],
  "dependencies": {
    "ws": "^7.2.5",
    "bluebird": "^3.7.2",
    "chalk": "^4.0.0",
    "forever": "^2.0.0",
    "events": "^3.1.0",
    "deribit-v2-ws-gitchrisqueen": "latest"
  },
  "devDependencies": {
    "jest": "^26.0.1"
  }
}
