{
  "name": "@gitchrisqueen/trading-app",
  "version": "0.1.1",
  "description": "Implementation of trading logic using Deribit data for BTC futures and TDAmeritrade for USD Futures",
  "main": "./src/app.js",
  "scripts": {
    "pretest": "npm install",
    "test": "jest",
    "testdebug": "node --trace-warnings --expose-gc node_modules/.bin/jest --no-cache --detectOpenHandles --runInBand",
    "testdebug2": "node --unhandled-rejections=strict --trace-warnings --expose-gc node_modules/.bin/jest --no-cache --detectOpenHandles --runInBand --debug --verbose",
    "unittestdebug": "node --unhandled-rejections=strict --trace-warnings --expose-gc node_modules/.bin/jest --no-cache --detectOpenHandles --runInBand --debug unit",
    "killall": "killall -v -9 node && killall -v -9 jest",
    "startdebug": "node -r dotenv/config --trace-warnings --expose-gc --max_old_space_size=4096 ./src/app.js",
    "startlocal": "node -r dotenv/config --max_old_space_size=4096 ./src/app.js",
    "start": "node ./src/app.js",
    "preversion": "npm run test",
    "version": "",
    "postversion": "git push && git push --tags && rm -rf build/temp",
    "postinstall": "node -e \"try{require('./postinstall')}catch(e){}\""
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/chris-queen-consulting"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/christopherqueenconsulting"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gitchrisqueen/trading-app.git"
  },
  "author": "Christopher Queen<christopher.queen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gitchrisqueen/trading-app/issues"
  },
  "homepage": "https://github.com/gitchrisqueen/trading-app#readme",
  "keywords": [
    "deribit",
    "tradingview",
    "crypto",
    "exchange",
    "bitcoin",
    "btc",
    "tdameritrade",
    "thinkorswim"
  ],
  "dependencies": {
    "@gitchrisqueen/tdameritrade-api-js-client": "^0.2.1",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "deribit-v2-ws-gitchrisqueen": "^1.0.25",
    "events": "^3.2.0",
    "express": "^4.17.1",
    "isomorphic-ws": "^4.0.1",
    "morgan": "^1.10.0",
    "serve-static": "^1.14.1",
    "urldecode": "^1.0.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "babel-jest": "^26.2.2",
    "dotenv": "^8.2.0",
    "jest": "^26.2.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2"
  }
}
